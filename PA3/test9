PROGRAM circle
	!Illegal type for an If condition
	REAL :: r=3, a, p, b =2	
	character (LEN = 20) :: str1 = "Hello World!", str2
	a = (3.14) * r * r
	IF ( r ) THEN
	  p = 2 * 3.14 * b 
	END IF
	PRINT *, "Results: ", r, " ", b, " ", a, " ", p
	print *, str1, "str2: ", str2, r
END PROGRAM circle
