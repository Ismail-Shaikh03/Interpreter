PROGRAM circle
	!Testing Block-If Else-clauseand string assignment
	REAL :: r, a, p
	integer:: b = 2	
	CHARACTER (LEN = 4):: str1, str2
	
	r = 3
	a = (3.14) * r * r
	IF ( r == 5) THEN
	  p = 2 * 3.14 * b 
	  print *, a, p, r
	else
		str1 = "CS"
		str2 = "Program"
		print *, str1, str2, " ", b
	END IF
END PROGRAM circle
